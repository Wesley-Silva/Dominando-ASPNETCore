@model IList<ASPNETCoreMVC.Models.SocialIcon>

<div class="col-md-12" style="padding-top:20px;">
    @foreach (var icon in Model)
    {
        <a style="background:@icon.IconBgColor; text-decoration:none" href="@icon.IconTargetUrl">
            <i class="@icon.IconClass"></i>
            @icon.IconName
        </a>
    }
</div>